{
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ],
    "env": {
      "PORT": "8000"
    },
    "config": {
      "start": "gunicorn -w 4 -b 0.0.0.0:$PORT app:app"
    }
  }
  